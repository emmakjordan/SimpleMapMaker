import java.util.*;
import java.io.*;
import java.awt.*;

/**
 * Nodes with coordinates for display
 *
 * @author Emma Jordan
 * @version 12/7/16
 *
 */
public class DisplayNode extends Graph.Node {
    
    /** Point coordinates for display */
    private Point point;

    /** Constructor */
    DisplayNode(V data, Point point) {
	this.data = super(new V(data));
	this.point = point;
    }

    /** Accessor for point */
    public Point getPoint() {
	return this.point;
    }

    /** Manipulator for point */
    public void setPoint(Point point) {
	this.point = point;
    }
}